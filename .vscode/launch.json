{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "LOCAL",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "npx",
      "runtimeArgs": ["ts-node"],
      "program": "${workspaceFolder}/src/index.ts",
      "preLaunchTask": "tsc: watch - tsconfig.json",
      "envFile": "${workspaceFolder}/.env/local.env",
      "sourceMaps": true,
      "restart": true
    }
  ],
  "compounds": []
}
